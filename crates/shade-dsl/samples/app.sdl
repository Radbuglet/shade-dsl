pub const bool = lang("bool");

pub const Option = fn<ty: type>() -> type {
    enum {
        Some: ty;
        None: ();

        pub const unwrap = fn(self: Self) -> ty {
            match self {
                new Some(v) => v,
                new None => panic("attempted to unwrap None"),
            }
        };
    }
};

