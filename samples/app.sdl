const get_builtin_type = intrinsic("core.get_builtin_type");

const bool = get_builtin_type("bool");
const u32 = get_builtin_type("u32");
const metatype = get_builtin_type("type");

const whee = fn<T: metatype>(v: T) -> V {
    const V = type((metatype,));

    (T,)
};

const maz = whee.<bool>(faz);
const faz = true;
