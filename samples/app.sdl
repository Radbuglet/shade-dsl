const get_builtin_type = intrinsic("core.get_builtin_type");

const bool = get_builtin_type("bool");
const u32 = get_builtin_type("u32");
const metatype = get_builtin_type("type");

const whee = fn<T: metatype>((v, v2): (T, T)) -> V {
    const V = type(((metatype,), T));

    let foo = type(());
    let bar = foo;

    let (baz, maz) = (foo, bar);
    let baz_ = maz;
    let maz_ = baz_;

    ((maz_,), v)
};

const maz = whee.<bool>((faz, faz));
const faz = true;
